config_version=4

[application]

config/name="Reds Tale"
config/description="A platformer about a girl named red, she falls into a cave and must escape by going deeper into the cave."
run/main_scene="res://Scenes/MainMenu.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="RedsTale"
run/delta_smoothing=false
config/icon="res://Assets/Art/icon.png"

[autoload]

SaveManager="*res://Scripts/SaveManager.gd"
GameManager="*res://Scripts/GameManager.gd"
MusicController="*res://Scenes/AutoLoads/MusicController.tscn"
LevelChanger="*res://Scripts/LevelChanger.gd"
ResourceManager="*res://Scripts/ResourceManager.gd"

[display]

window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

fps=false

